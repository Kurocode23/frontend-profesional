{
  "name": "platzigram-api",
  "version": "1.0.0",
  "description": "Platzigram REST API",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime",
      "transform-async-to-generator"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "ava": {
    "files": [
      "test/*.js",
      "!**/stub"
    ],
    "require": [
      "babel-register"
    ]
  },
  "scripts": {
    "lint": "standard --fix",
    "test": "npm run lint && NODE_ENV=test ava"
  },
  "author": "Kurocode <alanaliaga23@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-eslint": "^7.1.1",
    "babel-register": "^6.23.0",
    "standard": "^8.6.0",
    "test-listen": "^1.0.1"
  },
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "http-hash": "^2.0.0",
    "jsonwebtoken": "^7.3.0",
    "micro": "^7.0.6",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "token-extractor": "^0.1.6",
    "uuid-base62": "^0.1.0"
  }
}
